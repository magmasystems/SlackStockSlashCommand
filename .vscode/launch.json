    "version": "0.2.0",
    "configurations": [
        {
            "name": "Local Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/application.go",
            "args": [ "env", "local" ]
        },
        {
            "name": "AWS Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/application.go",
            "args": []
        },
        {
            "name": "Debug Delve",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}",
            "showLog": true
        },
        {
            "name": "Test",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceRoot}",
            "showLog": true
        },

    ]
}